<h3>Products</h3>

<div class="row">
  <div class="section">
    <% @products.each do |product| %>
      <div class="col s4">
        <h4><%= link_to product.name, product_path(product.id), remote: true %></h4>
        <span id="product-detail-<%= product.id %>"></span>
      </div>
    <% end %>
  </div>
</div>
